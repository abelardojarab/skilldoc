(((title "Quality and Reliability (QRE) Application Program Interface (API) documentation") (author "QRE team") (affiliation "LTD @ Intel Corp.") (abstract (ast "div" ("This document introduces documentation for the FRAT API functions.") () double xhtml10-transitional ())) (scheme-source-file . "/p/advda/elisp/utils/doctool/doctooloutput/lookupfunc.scm/") (laml-resource . "false") (documentation-commenting-style . "multi-semicolon") (default-attribute-explanation . not-provided) (source-destination-delta . "") (css-prestylesheet . "normal") (css-stylesheet . "fancy") (css-stylesheet-copying . "true") (make-dtd-manual-template . not-provided) (keep-syntactical-comment-file . "false") (manual-destination-name . "lookupfunc") (mirror-names-defined . not-provided) (mirror-name-prefix . not-provided) (attribute-sorting . not-provided) (scheme-source-linking . "true")) ((kind "manual-section") (section-title "Document parsing/generation functions - This section contains the XML/JSON/YAML document parsing/generation functions") (section-body "") (section-id "xml-load")) ((cross-references (reference "Scheme source file" "TcfXmlLoad" "lookupfunc-ep/lookupfunc-1.html#TcfXmlLoad")) (kind "manual-page") (description "Head of each list is the name of the element, and the (cadr) is a DPL containing any attribute-value pairs. Rest of each list is all the text contents or sub-elements. inputXml file is generated using: xsltproc TcfXmlData.xsl TcfXmlData.xml > TcfXmlData.out ") (title "TcfXmlLoad") (form TcfXmlLoad)) ((cross-references (reference "Scheme source file" "TcfXmlFind" "lookupfunc-ep/lookupfunc-1.html#TcfXmlFind")) (kind "manual-page") (description "Function to perform multiple-tag search on a XML data structure ") (title "TcfXmlFind") (form TcfXmlFind)) ((cross-references (reference "Scheme source file" "TcfXmlFindTagBFS" "lookupfunc-ep/lookupfunc-1.html#TcfXmlFindTagBFS")) (kind "manual-page") (description "Function to prune part of the XML tree based on the XML tag Uses BFS (breadth first search) to find the XML tag. ") (title "TcfXmlFindTagBFS") (form TcfXmlFindTagBFS)) ((cross-references (reference "Scheme source file" "TcfXml" "lookupfunc-ep/lookupfunc-1.html#TcfXml")) (kind "manual-page") (parameters (parameter "addTags" "variable number of XML entry tags")) (examples (example "<pre>\nExample: \nxml=TcfXml(\"ROOT\" \"DATA\" \"INFO\") \nxmlFile=outfile(\"out.xml\") \nfprintf(xmlFile \"%s\" xml->start_xml( obj )) \nfprintf(xmlFile \"%s\\n\" \n                xml->ROOT( \n                          ?hello \"world\" \n                          ?attribute \"value\" \n                          xml->DATA( \n                                xml->INFO(?myAttrib \"val1\") \n                                ) \n                         ) \n                ) \nclose(xmlFile) \n\nThis produces output like this: \n<?xml version=\"1.0\" encoding=\"UTF-8\"?> \n<ROOT hello=\"world\" attribute=\"value\"><DATA><INFO myAttrib=\"val1\"/></DATA></ROOT> \n</pre>")) (description "Main function which operates over the TcfXmlObj data structure. Better support for XML (both HTML and XML are handled with the same API). (can specify whatever tags you like when you call TcfXml - e.g. TcfXml(\"tag1\" \"tag2\" \"tag3\")). Added a ->start_xml method. Also supports tags with no content. ") (form "TcfXml (@rest addTags)") (title "TcfXml")) ((cross-references (reference "Scheme source file" "htmlTag" "lookupfunc-ep/lookupfunc-1.html#htmlTag")) (level 2) (kind "manual-page") (description "Internal function that is used for most of the tags. This processes all the keyword arguments, and builds the tag parameters. Also supports special boolean arguments ?begin and ?end to indicate that just the beginning and end tag should be generated. The default is both. ") (title "htmlTag") (form htmlTag)) ((kind "manual-section") (section-title "Get/fetch functions - This section contains the get/fetch functions") (section-body "") (section-id "get-load")) ((cross-references (reference "Scheme source file" "TcfGetPolyPitchID" "lookupfunc-ep/lookupfunc-1.html#TcfGetPolyPitchID")) (kind "manual-page") (parameters (parameter "csvfile" "absolute filename for the CSV file containing the matching between numerical polyPitch value and polyPitch ID LPP")) (description "Returns the LPP for the correct poly pitch ") (form "TcfGetPolyPitchID (string @key (csvfile \"/p/adx/x74y/mounts/projects/da_projects/x74y/utils/tcc_collateral/1274/pCells/SCRIBE/COMMON/FramePolyPitchID.csv\"))") (returns "idLPP (LPP value)") (title "TcfGetPolyPitchID")) ((cross-references (reference "Scheme source file" "TcfGetPlugValues" "lookupfunc-ep/lookupfunc-1.html#TcfGetPlugValues")) (kind "manual-page") (description "Returns the defaults for plug layers. In keeping with the current thought process - The sizing values returned are for sizing hte feature before inverting for plugs Given \"layer\" \"pluglyer\" \"xsizing\" \"ysizing\" where the users only hvae to pass values if layet want to over ride TcfGetPlugValues \"m0gpc\" \"Default\" \"Default\" \"Default\" Or TcfGetPlugValues \"mogpc\" \"metal0.plug\" 0.004 0.05 the function will return a list ((metal0 plug) 00.004 0.05) ") (title "TcfGetPlugValues") (form TcfGetPlugValues)) ((cross-references (reference "Scheme source file" "TcfGetLPP" "lookupfunc-ep/lookupfunc-1.html#TcfGetLPP")) (kind "manual-page") (description "returns the LPP of a given frame layer if givi=en layer.purpose - returns (layer purpose) if givien a single string - looks into the mapping file for the matching LPP and returns that ") (title "TcfGetLPP") (form TcfGetLPP)) ((cross-references (reference "Scheme source file" "TcfGetSDRLppList" "lookupfunc-ep/lookupfunc-1.html#TcfGetSDRLppList")) (kind "manual-page") (description "Modifications for the lack of TCC_CONTROLS_FILE ") (title "TcfGetSDRLppList") (form TcfGetSDRLppList)) ((cross-references (reference "Scheme source file" "TcfGetSDRWXPropList" "lookupfunc-ep/lookupfunc-1.html#TcfGetSDRWXPropList"))